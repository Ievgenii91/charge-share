{
  "kind": "collectionType",
  "collectionName": "providers",
  "info": {
    "singularName": "provider",
    "pluralName": "providers",
    "displayName": "Provider",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "address": {
      "type": "string"
    },
    "isInUse": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "isIdle": {
      "type": "boolean",
      "default": false
    },
    "rating": {
      "type": "integer",
      "default": 5,
      "max": 5,
      "min": 0
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "power": {
      "type": "integer"
    },
    "chargingHours": {
      "type": "integer"
    },
    "geoProviderAddress": {
      "type": "json"
    },
    "isInUseBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::consumer.consumer"
    },
    "lastChargeStartDate": {
      "type": "datetime"
    },
    "lastChargeEndDate": {
      "type": "datetime"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "providers"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    }
  }
}
